{
  "name": "provider/iana",
  "description": "IANA data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "connectors/http": "dev-5.0-rc",
    "connectors/ssl": "dev-5.0-rc",
    "league/csv": "^8",
    "scriptfusion/porter": "dev-master as 5",
    "symfony/dom-crawler": "^3"
  },
  "require-dev": {
    "mockery/mockery": "^1",
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Iana\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Iana\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test"
  },
  "config": {
    "sort-packages": true
  }
}
